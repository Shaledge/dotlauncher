        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    
    <Grid>
        <Button Name="cmdAdd" Content="Add" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" RenderTransformOrigin="2.867,11.364" Margin="157,275,0,0" Click="cmdAdd_Click"  />
        <Button  Name="cmdLaunch" Content="Launch" HorizontalAlignment="Left" Margin="308,275,0,0" VerticalAlignment="Top" Width="75" Click="cmdLaunch_Click"/>
        <ListBox Name="lstUrls" HorizontalAlignment="Left" Margin="10,10,10,100" VerticalAlignment="Top" RenderTransformOrigin="1.87,0.74" KeyDown="lstUrls_KeyDown"/>
    </Grid>
</Window>
